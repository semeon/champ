<table ng-show="items.length" class="table table-condensed table-hover table-striped">
	<thead>
		<tr>
			<th>Имя</th>
			<th>Ник</th>
			<th>Email</th>
			<th>Телефон</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<tr ng-repeat="item in items">
			<td>{{item.name}}</td>
			<td><em>{{item.nick}}</em></td>
			<td>{{item.email}}</td>
			<td>{{item.tel}}</td>
			<td width="100px;">
				<% include __item_controls.ejs %>
			</td>
		</tr>
	</tbody>
</table>
